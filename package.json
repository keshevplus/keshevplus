{
  "name": "keshev-web",
  "version": "1.0.0",
  "description": "Keshev Plus - Fullstack Application",
  "main": "server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "node server.js",
    "dev:client": "cd client && pnpm run dev",
    "build:all": "cd client && pnpm run build && pnpm run start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@neondatabase/serverless": "^1.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "faker": "5.5.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodejs": "^0.0.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.1",
    "pnpm": "^10.11.0",
    "react-hot-toast": "^2.5.2",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
